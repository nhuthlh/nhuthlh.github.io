<%- include("partials/header"); -%>

<h1>Compose</h1>
<form action="/compose" method="post">
    <div class="form-group">
        <div class="mb-3">
            <label for="title" class="form-label">Title</label><br>
            <input class="form-control" id="itle" type="text" name="postTitle" autocomplete="off" style="width:100%"><br>
        </div>
        <div class="mb-3">
            <label for="post" class="form-label">Post</label><br>
            <textarea class="form-control" id="post" name="postBody" rows="5" autocomplete="off" style="width:100%""></textarea><br>
    </div>
    </div>
   
    <button class=" btn btn-primary" type="submit" name="button">Publish</button>
</form>

<%- include("partials/footer"); -%>